{
    "notify_no_data": false,
              "name": "<%= environment %> Bosh: Running out of system disk",
          "silenced": false,
             "query": "avg(last_10m):avg:bosh.healthmonitor.system.disk.system.percent{deployment:<%= bosh_deployment %>} by {job,index} > 90",
             "message": "Healthmonitor reports system disk is > 90% full on {{job.name}}/{{index.name}}. <%= cloudops_non_critical_pagerduty %>. Escalation Path: It depends on the job."
}
