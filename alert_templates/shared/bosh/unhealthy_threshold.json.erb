{
  "escalation_message": "",
  "message": "## Description:\n >20% of the BOSH agents are reporting either that the VM that they are running on or the job that they are running is unhealthy. <%= cloudops_pagerduty %>.  \n\n## Escalation Path:\n  It depends on the job. \n\n## Possible Causes: Is there a deployment going on?\n## Potential Solutions:\n\n##Feedback:\n Have feedback for this alert? [File an issue](https://github.com/pivotal-cf/datadog-config/issues/new)",
  "name": "<%= environment %> BOSH Agent reporting as unhealthy below threshold",
  "no_data_timeframe": false,
  "notify_audit": true,
  "notify_no_data": false,
  "query": "avg(last_5m):avg:bosh.healthmonitor.system.healthy{!deployment:<%= metron_agent_deployment %>-jarvice-playground-v2,!deployment:p-gemfire,!deployment:<%= jarvice_deployment %>} by {deployment,job} < 0.8",
  "renotify_interval": null,
  "timeout_h": null
}