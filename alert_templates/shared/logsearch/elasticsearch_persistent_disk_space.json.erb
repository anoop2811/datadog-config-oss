  "silenced": false,
  "query": "avg(last_5m):max:bosh.healthmonitor.system.disk.persistent.percent{!job:curator_z1,!job:curator_z2,!job:curator_z3,!job:elb_log_to_logstash_z2,!job:ingestor_z1,!job:ingestor_z2,!job:ingestor_z3,!job:log_parser_z1,!job:log_parser_z2,!job:log_parser_z3,!job:metrics_collector_z1,!job:metrics_collector_z2,!job:metrics_collector_z3,!job:queue_z1,!job:queue_z2,!job:queue_z3,deployment:cf-elk-3az} > 90",
