{
  "notify_no_data": true,
  "no_data_timeframe": 20,
  "name": "<%= environment %> Logsearch: ElasticSearch is not ingesting messages",
  "silenced": false,
  "query": "max(last_30m):min:elasticsearch.docs.incomingRatePerSecond{environment:<%= environment %>} < 1",
  "message": "In the last thirty minutes, the ElasticSearch document count did not grow on at least one instance or we did not receive any data. <%= monitoringAndMetrics_email %>"
}
