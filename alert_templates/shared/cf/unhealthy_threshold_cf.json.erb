{
    "notify_no_data": false,
              "name": "<%= environment %> CF: BOSH Agent reporting as unhealthy below threshold",
          "silenced": false,
             "query": "avg(last_5m):avg:bosh.healthmonitor.system.healthy{deployment:<%= deployment %>} by {job} < 0.8",
           "message": "One or more of the BOSH agents are reporting either that the VM that they are running on or the job that they are running is unhealthy. <%= backend_pagerduty %>"
}
