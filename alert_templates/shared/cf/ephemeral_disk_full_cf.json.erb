{
  "notify_no_data": false,
  "name": "<%= environment %> CF: A job is running out of ephemeral disk",
  "silenced": false,
  "query": "avg(last_5m):max:bosh.healthmonitor.system.disk.ephemeral.percent{deployment:<%= deployment %>} by {job,index} > 80",
  "message": "One of the components has exceeded 80% ephemeral disk usage. <%= backend_pagerduty %>"
}
