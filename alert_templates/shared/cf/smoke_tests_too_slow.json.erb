{
  "notify_no_data": false,
  "name": "<%= environment %> CF: Smoke Tests are too slow",
  "silenced": false,
  "query": "avg(last_30m):avg:smoke.execution_time_ms{deployment:<%= deployment %>,loggregator_enabled:0} > <%= params.fetch('smoke_timeout_secs') * 1000 %>",
