{
  "notify_no_data": true,
  "name": "<%= environment %> CF: AWS has instance on ap-southeast-1 (usally fraudulent incident)",
  "silenced": false,
  "query": "max(last_5m):max:aws.ec2.cloudops.instance_count{region:ap-southeast-1} > 0",
  "message": "This alert should only be triggered when AWS has instance in ap-southeast-1 <%= cloudops_email %>"
}

