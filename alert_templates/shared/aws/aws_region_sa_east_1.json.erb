{
  "notify_no_data": true,
  "name": "<%= environment %> CF: AWS has instance on sa-east-1 (usally fraudulent incident)",
  "silenced": false,
  "query": "max(last_5m):max:aws.ec2.cloudops.instance_count{region:sa-east-1} > 0",
  "message": "This alert should only be triggered when AWS has instance in sa-east-1 <%= cloudops_email %>"
}

