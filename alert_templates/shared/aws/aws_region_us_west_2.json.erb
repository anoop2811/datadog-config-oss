{
  "notify_no_data": true,
  "name": "<%= environment %> CF: AWS has instance on  us-west-2 (usally fraudulent incident)",
  "silenced": false,
  "query": "max(last_5m):max:aws.ec2.cloudops.instance_count{region:us-west-2} > 1",
  "message": "This alert should only be triggered when AWS has more than 1 instance in us-west-2 <%= cloudops_email %>"
}

