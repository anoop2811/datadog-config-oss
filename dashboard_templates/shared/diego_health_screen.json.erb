      "title": "Cell: Tasks",
      "definition":{
        "requests":[
          {
            "q":"avg:datadog.nozzle.runtime_metrics_server.TasksPending{deployment:<%= deployment %>}"
          },
          {
            "q":"avg:datadog.nozzle.runtime_metrics_server.TasksClaimed{deployment:<%= deployment %>}"
          },
          {
            "q":"avg:datadog.nozzle.runtime_metrics_server.TasksRunning{deployment:<%= deployment %>}"
          },
          {
            "q":"avg:datadog.nozzle.runtime_metrics_server.TasksCompleted{deployment:<%= deployment %>}"
          },
          {
            "q":"avg:datadog.nozzle.runtime_metrics_server.TasksResolving{deployment:<%= deployment %>}"
          }
        ],
        "events":[
          {
            "q": "tags:deployment:<%= diego_deployment %> started deploying diego to <%= deployment %>"
          },
          {
            "q": "tags:deployment:<%= diego_deployment %> finished deploying diego to <%= deployment %>"
          },
