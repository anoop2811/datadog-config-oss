          {
            "q": "datadog.nozzle.runtime_metrics_server.CrashedActualLRPs{deployment: <%=deployment %>} + datadog.nozzle.runtime_metrics_server.LRPsStarting{deployment: <%=deployment %>} + datadog.nozzle.runtime_metrics_server.LRPsRunning{deployment: <%=deployment %>}"
