{
  "graphs": [
    {
      "definition": {
        "requests": [
          {
            "q": "avg:nyet.create.response_time_ms{deployment:<%= deployment %>} by {app_type}",
			"stacked": false
          }
        ],
        "events": [<%= default_events_json %>]
      },
      "title": "Time (ms) for app to be 'created'"
    },
    {
      "definition": {
        "requests": [
