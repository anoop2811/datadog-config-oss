      "definition": {
        "requests": [
          {
            "q": "aws.ec2.cpuutilization{deployment:<%= deployment %>} by {host}"
          }
        ],
        "events": [
          {
            "q": "*"
          }
        ]
      },
      "title": "CPU usage, by host (%)"
    },
    {
