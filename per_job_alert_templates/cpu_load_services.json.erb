<% skip_cf %>
{
  "notify_no_data": false,
  "name": "<%= environment %> Services: <%= job %> job is reporting a high CPU load average",
  "silenced": false,
  "query": "avg(last_10m):avg:bosh.healthmonitor.system.load.1m{deployment:<%= services_deployment %>,job:<%= job %>} by {index} > 5",
  "message": "Healthmonitor reports CPU load above 5 in one of the <%= job %> VMs. <%= services_pagerduty %>"
}
